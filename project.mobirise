ty\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn align-center pb-3\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-sm btn-secondary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"><span class=\"socicon socicon-instagram mbr-iconfont mbr-iconfont-btn\"></span>FOLLOW US</a></div>\n        <div class=\"row\">\n\n\n\n            <div class=\"item col-6 col-md-3 col-lg-x\" mbr-if=\"items > 0\">\n                <div class=\"img-wrap\">\n                    <a href=\"Servicios.html\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-btn mdi-image-remove-red-eye\"></span></a>\n                    <img src=\"@PROJECT_PATH@/assets/images/17-253x253.jpg\" alt>\n                </div>\n            </div>\n\n            <div class=\"item col-6 col-md-3 col-lg-x\" mbr-if=\"items > 1\">\n                <div class=\"img-wrap\">\n                    <a href=\"Servicios.html\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-btn mdi-image-remove-red-eye\"></span></a>\n                    <img src=\"@PROJECT_PATH@/assets/images/18-253x253.jpg\" alt=\"Mobirise\">\n                </div>\n            </div>\n            <div class=\"item col-6 col-md-3 col-lg-x\" mbr-if=\"items > 2\">\n                <div class=\"img-wrap\">\n                    <a href=\"Servicios.html\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-btn mdi-image-remove-red-eye\"></span></a>\n                    <img src=\"@PROJECT_PATH@/assets/images/19-253x253.jpg\" alt>\n                </div>\n            </div>\n            <div class=\"item col-6 col-md-3 col-lg-x\" mbr-if=\"items > 3\">